<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :artist_id%>
  <%= f.text_field :artist_id %>
  <br/>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <input name="song[contents][]" id = "song_notes_1">
  <input name="song[contents][]" id = "song_notes_2">
  <input name="song[contents][]" id = "song_notes_3">

  <%= f.submit %>
<% end %>


